@model Telerik.Sitefinity.Frontend.Mvc.Helpers.ViewModels.Fields.MediaFieldViewModel

@using Telerik.Sitefinity.Frontend.Mvc.Helpers; 

<div>
    @if (!Model.FieldTitle.IsNullOrEmpty())
    {
        <strong>@Model.FieldTitle</strong>
    }
    <ul>
        @foreach(var contentLink in Model.MediaItems)
        {
            <li>
                <video controls>
                    <source  src='@contentLink.GetVideo().MediaUrl'>
                </video>
            </li>
        }
    </ul>
</div>